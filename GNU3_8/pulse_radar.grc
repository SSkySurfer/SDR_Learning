  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [338, 2]
    rotation: 0
    state: true
- name: tgt_pw_tau
  id: variable
  parameters:
    comment: ''
    value: round(tgt_pw/(1/samp_rate))
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [706, 6]
    rotation: 0
    state: true
- name: track_spacing
  id: variable
  parameters:
    comment: ''
    value: round((tgt_pri/tgt_pw)*((100-percent_tracks)/100)*(tgt_pw_tau))
